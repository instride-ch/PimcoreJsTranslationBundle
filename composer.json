{
  "name": "w-vision/pimcore-js-translation-bundle",
  "type": "pimcore-bundle",
  "license": "GPL-3.0-or-later",
  "description": "A pretty nice way to expose your Pimcore shared translation messages to your client applications.",
  "keywords": [
    "pimcore",
    "pimcore-bundle",
    "translations",
    "pimcore-translations",
    "javascript-translations"
  ],
  "homepage": "https://github.com/w-vision/PimcoreJsTranslationBundle",
  "authors": [
    {
      "name": "w-vision AG",
      "email": "support@w-vision.ch",
      "homepage": "https://www.w-vision.ch",
      "role": "Web Development Agency"
    }
  ],
  "require": {
    "coreshop/resource-bundle": "^2.0",
    "willdurand/js-translation-bundle": "^2.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Wvision\\Bundle\\PimcoreJsTranslationBundle\\": "src/PimcoreJsTranslationBundle/"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "Wvision\\Bundle\\PimcoreJsTranslationBundle\\PimcoreJsTranslationBundle"
      ]
    },
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
